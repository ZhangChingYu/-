}

/* 商品描述 */
.goods-message.data-v-12dbf08d {
	margin: 0 auto;
	display: flex;
	flex-direction: column;
	width: 95%;
	/* height: 35rem; */
	overflow: hidden;
	background-color: #eeeeee;
	border-radius: 1rem 1rem 0 0;
}

/* 商品名称 */
.goods-message .goods-title.data-v-12dbf08d {
	margin: 0.88rem;
	/* height: 3rem; */
	max-height: 4.5rem;
	font-size: 1rem;
	font-weight: 600;
	line-height: 1.5rem;
}
.goods-message .goods-describe.data-v-12dbf08d {
	margin: 0 0.48rem 0.68rem;
	max-height: 2.5rem;
	text-indent: 2em;
	line-height: 1.25rem;
	overflow: hidden;
}
.goods-message .goods-pic.data-v-12dbf08d {
	text-align: center;
	flex: 1;
}

/* 商品图片 */
.swiper.data-v-12dbf08d {
	height: 60vw;
}
.goods-pic .swiper-item image.data-v-12dbf08d {
	width: 90%;
	height: 60vw;
}

/* 其他信息 */
