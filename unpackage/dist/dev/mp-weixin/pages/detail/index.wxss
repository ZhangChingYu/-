@font-face {
  font-family: 'iconfont';
  src: url(data:font/woff2;base64,) format('woff2'), url(data:font/woff;base64,) format('woff'), url(data:font/ttf;base64,) format('truetype');
}
.iconfont.data-v-12dbf08d {
  margin: 0 0.38rem;
  font-family: "iconfont" !important;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.detail.data-v-12dbf08d {
  min-height: 100vh;
}
/* 顶部基本信息 */
.top-message.data-v-12dbf08d {
  overflow: hidden;
}
.top-message .saller.data-v-12dbf08d {
  margin-left: 60rpx;
  display: flex;
  align-items: center;
  height: 180rpx;
  width: 60%;
}
.saller .avatar.data-v-12dbf08d {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}
.saller .saller-name.data-v-12dbf08d {
  margin-left: 15rpx;
  display: inline-block;
  font-size: 30rpx;
  font-weight: 600;
  line-height: 150rpx;
}
/* 价格 */
.saller-price.data-v-12dbf08d {
  position: absolute;
  top: 30rpx;
  right: 50rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  font-size: 40rpx;
}
.saller-price .sprice.data-v-12dbf08d {
  margin: 15rpx auto;
  color: red;
}
/* 发布时间地点 */
.publish-message.data-v-12dbf08d {
  margin: 0 40rpx;
  line-height: 60rpx;
  font-size: 24rpx;
  color: #b9b9b9;
}
.publish-message .publish-time.data-v-12dbf08d {
  float: left;
  margin: 0 20rpx;
}
.publish-message .publish-address.data-v-12dbf08d {
  float: left;
}
/* 商品描述 */
.goods-message.data-v-12dbf08d {
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  width: 95%;
  overflow: hidden;
  background-color: #eeeeee;
  border-radius: 20rpx 20rpx 0 0;
}
/* 商品名称 */
.goods-message .goods-title.data-v-12dbf08d {
  margin: 20rpx;
  max-height: 100rpx;
  font-size: 36rpx;
  font-weight: 600;
  line-height: 1.5em;
  word-break: break-all;
  overflow: hidden;
  -webkit-line-clamp: 2;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
}
.goods-message .goods-describe.data-v-12dbf08d {
  margin: 10rpx;
  max-height: 170rpx;
  text-indent: 2em;
  line-height: 1.5em;
  font-size: 24rpx;
  word-break: break-all;
  overflow: hidden;
  -webkit-line-clamp: 5;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
}
.goods-message .goods-pic.data-v-12dbf08d {
  text-align: center;
  flex: 1;
}
/* 商品图片 */
.swiper.data-v-12dbf08d {
  height: 60vw;
}
.goods-pic .swiper-item image.data-v-12dbf08d {
  width: 90%;
  height: 60vw;
}
/* 其他信息 */
.goods-otherMsg.data-v-12dbf08d {
  margin-top: 20rpx;
  height: 50rpx;
  line-height: 50rpx;
}
.goods-otherMsg view.data-v-12dbf08d:not(.report) {
  float: right;
  margin: 0 30rpx;
  font-size: 28rpx;
  color: #9d9d9d;
}
/* 举报 */
.report.data-v-12dbf08d {
  /* position: fixed; */
  float: left;
  margin-left: 50rpx;
  width: 80rpx;
  border-radius: 10rpx;
  background-color: #d0cecf;
  color: #fff;
  font-size: 24rpx;
  text-align: center;
}
/* 举报 */
.oneState.data-v-12dbf08d {
  display: flex;
  flex-direction: column;
  text-align: center;
}
.subLine.data-v-12dbf08d {
  background-color: #b34c26;
  width: 66%;
  height: 6rpx;
  margin-left: 17%;
}
.oneState > text.data-v-12dbf08d {
  height: 80rpx;
  line-height: 80rpx;
  padding: 10rpx;
  font-size: 26rpx;
}
/* 留言 */
.evaluation.data-v-12dbf08d {
  margin-bottom: 120rpx;
}
.evaluation ._h3.data-v-12dbf08d {
  margin: 20rpx 0;
  padding-left: 40rpx;
  height: 50rpx;
  font-size: 30rpx;
  color: #fff;
  background: linear-gradient(to right, #c13535, #ffd5c0);
  border-top: 2px solid #9d9d9d;
}
.add-evaluation.data-v-12dbf08d {
  display: flex;
  margin: 0 auto;
  width: 650rpx;
}
.add-evaluation .send-input.data-v-12dbf08d {
  width: 500rpx;
}
.add-evaluation .send-btn.data-v-12dbf08d {
  width: 150rpx;
}
/* 底部操作部分 */
.footer-operator.data-v-12dbf08d {
  position: -webkit-sticky;
  position: sticky;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  align-items: center;
  bottom: 0;
  width: 100%;
  height: 120rpx;
  font-size: 32rpx;
  line-height: 84rpx;
  background-color: #ffffff;
}
.footer-operator .iconfont.data-v-12dbf08d {
  font-size: 1.2rem;
}
.hasLiake.data-v-12dbf08d {
  color: red;
}
.footer-operator .like.data-v-12dbf08d,
.footer-operator .notlike.data-v-12dbf08d,
.footer-operator .contact.data-v-12dbf08d {
  display: flex;
  justify-content: center;
  width: 200rpx;
}
.footer-operator .book-goods.data-v-12dbf08d {
  width: 300rpx;
  background-color: #c13535;
  border-radius: 0.5rem;
  color: #fff;
  text-align: center;
}
/* 预约弹窗 */
.popup-content.data-v-12dbf08d {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 50vw;
  font-size: 36rpx;
  line-height: 84rpx;
}
.popup-content .book-count.data-v-12dbf08d {
  width: 50%;
  height: 84rpx;
  display: flex;
}
.popup-content .book-count label.data-v-12dbf08d {
  display: inline-block;
  width: 3em;
}
.popup-content .book-count input.data-v-12dbf08d {
  flex: 1;
  padding-left: 1em;
  box-sizing: border-box;
  height: 84rpx;
  border: 4rpx solid #d0cecf;
  border-radius: 10rpx;
}
.popup-content .book-submit.data-v-12dbf08d {
  margin-top: 50rpx;
  width: 50%;
  height: 84rpx;
  max-width: 200px;
  max-height: 50px;
  background-color: #c13535;
  color: #fff;
  border-radius: 15rpx;
  text-align: center;
}
/* 没有留言 */

