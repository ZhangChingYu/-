	font-size: 1.36rem;
}
.saller-price .sprice.data-v-12dbf08d {
	margin: 0.48rem auto;
	color: red;
}

/* 发布时间地点 */
.publish-message.data-v-12dbf08d {
	margin:0 1rem;
	height: 3rem;
	/* width: 60%; */
	line-height: 3rem;
	font-size: 0.88rem;
	color: #b9b9b9;
}
.publish-message .publish-time.data-v-12dbf08d{
