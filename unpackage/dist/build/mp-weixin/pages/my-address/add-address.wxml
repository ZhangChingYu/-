<view class="add-address"><button data-event-opts="{{[['tap',[['deleteAdd',['$event']]]]]}}" class="deleteBtn" bindtap="__e">删除该地址</button><view class="address-item"><view class="title">收件人</view><input type="text" value="{{addressObj.receiverName}}" placeholder="收件人姓名" data-event-opts="{{[['input',[['__set_model',['$0','receiverName','$event',[]],['addressObj']]]]]}}" bindinput="__e"/></view><view class="address-item"><view class="title">手机号</view><input type="text" value="{{addressObj.receiverPhone}}" placeholder="11位手机号" data-event-opts="{{[['input',[['__set_model',['$0','receiverPhone','$event',[]],['addressObj']]]]]}}" bindinput="__e"/></view><view class="address-item"><view class="title">所在地区</view><view data-event-opts="{{[['tap',[['showCityPicker',['$event']]]]]}}" bindtap="__e">{{addressObj.region+" >"}}</view><mpvue-city-picker class="vue-ref" vue-id="929f3f3e-1" pickerValueDefault="cityPickerValueDefault" data-ref="mpvueCityPicker" data-event-opts="{{[['^onConfirm',[['onConfirm']]]]}}" bind:onConfirm="__e" bind:__l="__l"></mpvue-city-picker></view><view class="address-item"><view class="title">详细地址</view><input type="text" value="{{addressObj.addressDetail}}" placeholder="5到60个字符" data-event-opts="{{[['input',[['__set_model',['$0','addressDetail','$event',[]],['addressObj']]]]]}}" bindinput="__e"/></view><view class="address-item"><view class="title">设为默认地址</view><radio-group><label class="radio"><radio color="#d04b41" checked="{{addressObj.isDefault}}" data-event-opts="{{[['tap',[['select']]]]}}" bindtap="__e"></radio><text></text></label></radio-group></view><button data-event-opts="{{[['tap',[['commit',['$event']]]]]}}" class="commitbtn" bindtap="__e">保存</button></view>